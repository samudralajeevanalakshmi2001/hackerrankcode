import math
import os
import random
import re
import sys
def conditional(N):
    if N % 2 != 0:
        print("Weird")
    elif N in range(2,6):
        print("Not Weird")
    elif N in range(6,21):
        print("Weird")
    else:
        print("Not Weird")
if __name__ == '__main__':
    N = int(input().strip())
    conditional(N)
